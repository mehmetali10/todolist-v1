<%- include("head")-%>
    <div class="box" id="heading">
        <h1><%= listTitle %> </h1>
    </div>

    <div class="box">
        <% for(var i =0; i<collection.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= collection[i] %></p>
            </div>
        <% } %>
        
        <form method="post" action="/">
            <input type="text" name="td" placeholder="New Item" autocomplete="off">
            <button type="submit" value=<%= listTitle %> name="list">+</button>
        </form>
    </div>

   
  
    <%- include("footer")-%>
       
   

